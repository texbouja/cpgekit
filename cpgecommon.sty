			\or \csdef{title@#2}{%
		\resizebox{\hsize}{!}{%
			\parbox{3\cpge@ttlw}{%
				\fil@inner@titlepage\linespread{.8}%
				#3\csuse{cpge@#2@format}\strut%
				\global\cpge@dima\baselineskip%
			}%
		}\baselineskip1.2\cpge@dima%
	}
			 \or 
			 \csdef{title@#2}{%
		\resizebox{\hsize}{!}{%
			\parbox{2\cpge@ttlw}{%
				\fil@inner@titlepage\linespread{.8}%
				#3\csuse{cpge@#2@format}\strut%
				\global\cpge@dima\baselineskip%
			}%
		}\baselineskip1.2\cpge@dima%
	}
			  \or \csdef{title@#2}{%
		\resizebox{\hsize}{!}{%
			\parbox{1.5\cpge@ttlw}{%
				\fil@inner@titlepage\linespread{.8}%
				#3\csuse{cpge@#2@format}\strut%
				\global\cpge@dima\baselineskip%
			}%
		}\baselineskip1.2\cpge@dima%
	}
				 \or \csdef{title@#2}{%
		\resizebox{\hsize}{!}{%
			\parbox{\cpge@ttlw}{%
				\fil@inner@titlepage\linespread{.8}%
				#3\csuse{cpge@#2@format}\strut%
